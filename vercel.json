{
  "buildCommand": "cd roam-admin-final/roam-admin && npm run build",
  "outputDirectory": "roam-admin-final/roam-admin/dist/spa",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
