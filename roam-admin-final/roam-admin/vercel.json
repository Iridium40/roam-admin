{
  // CHECKED: This file has been verified and contains the correct Vercel configuration
  "buildCommand": "npm run build:client",
  "outputDirectory": "dist/spa",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
