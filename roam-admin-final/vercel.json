{
  "buildCommand": "cd roam-admin && npm run build",
  "outputDirectory": "roam-admin/dist/spa",
  "installCommand": "cd roam-admin && npm install",
  "functions": {
    "roam-admin/api/**/*.ts": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/roam-admin/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
